package kr.ac.itschool.calculator;

import java.awt.BorderLayout;
import java.awt.Container;
import java.awt.Dimension;
import java.awt.event.ActionListener;

import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JPanel;
import javax.swing.JTextField;

public class CalculatorDemoMain {

	public static void main(String[] args) {
		JFrame frame=new JFrame("Calculator Js Program");
		frame.setLocation(500,400);
		frame.setPreferredSize(new Dimension(400,150));
		Container contentPane=frame.getContentPane();
		
		JPanel panel=new JPanel();
		JButton button1=new JButton("+");
		JButton button2=new JButton("-");
		JButton button3=new JButton("*");
		JButton button4=new JButton("/");
		JButton button5=new JButton("C");
		panel.add(button1);
		panel.add(button2);
		panel.add(button3);
		panel.add(button4);
		panel.add(button5);
		
		JPanel panel2=new JPanel();
		JTextField text=new JTextField(5);
		JLabel label2=new JLabel();
		JTextField text2=new JTextField(5);
		JTextField text3=new JTextField(10);
		JLabel label=new JLabel("=");	
		panel2.add(text);
		panel2.add(label2);
		panel2.add(text2);
		panel2.add(label);
		panel2.add(text3);
		
		contentPane.add(panel,BorderLayout.SOUTH);
		contentPane.add(panel2,BorderLayout.CENTER);
	
		ActionListener listener=	new CalculatorClickListener(text,text2,text3,button1,
				button2,button3,button4,button5,label2);
		ActionListener listener2=	new CalculatorClickListener2(text,text2,text3,button1,
				button2,button3,button4,button5,label2);
		ActionListener listener3=	new CalculatorClickListener3(text,text2,text3,button1,
				button2,button3,button4,button5,label2);
		ActionListener listener4=	new CalculatorClickListener4(text,text2,text3,button1,
				button2,button3,button4,button5,label2);
		ActionListener listener5=	new CalculatorClickListener5(text,text2,text3,button1,
				button2,button3,button4,button5,label2);
		button1.addActionListener(listener);
		button2.addActionListener(listener2);
		button3.addActionListener(listener3);
		button4.addActionListener(listener4);
		button5.addActionListener(listener5);
		frame.pack();
		frame.setVisible(true);
	}
}
